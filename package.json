    "nx": "nx",
    "start": "nx serve",
    "test": "nx test",
    "lint": "nx lint",
    "build": "npx nx build project-api",
    "dev": "nx run project-api:serve",
    "dev:nr": "PORT=4000 nx run national-registry:serve",
    "dev-init": "nx run project-api:dev-init && nx run national-registry:dev-init",
    "dev:services": "concurrently \"PORT=3000 nx run project-api:serve\" \"PORT=4000 nx run national-registry:serve\"",
    "openapi-generator": "openapi-generator generate -g typescript-fetch --additional-properties=typescriptThreePlus=true,enumPropertyNamingReplaceSpecialCharacters=true,allowUnicodeIdentifiers=true",
    "openapi-generator:test": "openapi-generator generate -g typescript-fetch -i tmp/swagger/client-config.json -o tmp/swagger/gen/fetch  --additional-properties=typescriptThreePlus=true,enumPropertyNamingReplaceSpecialCharacters=true,allowUnicodeIdentifiers=true",
    "codegen": "node scripts/codegen.js",
    "compile-strings:is": "formatjs compile ./apps/project-web/src/i18n/strings/is.json --ast --out-file ./apps/project-web/src/i18n/strings/is-compiled.json",
    "compile-strings": "concurrently \"pnpm:compile-strings:*\"",
    "extract-strings": "formatjs extract 'apps/project-web/src/**/*.{ts,tsx}' --ignore='**/*.d.ts' --out-file ./apps/project-web/src/i18n/strings/is.json && yarn compile-strings"
  },
