{
  "extends": ["../../../.eslintrc.json", "plugin:cypress/recommended"],
  "rules": {
    "no-prototype-builtins": "off",
    "@typescript-eslint/triple-slash-reference": "off"
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    { "files": ["*.ts", "*.tsx", "*.js", "*.jsx"], "rules": {} },
    { "files": ["*.ts", "*.tsx"], "rules": {} },
    { "files": ["*.js", "*.jsx"], "rules": {} }
  ]
}
