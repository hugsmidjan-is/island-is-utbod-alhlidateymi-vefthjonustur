  "components": {
    "schemas": {
      "AuthenticateModel": {
        "required": ["password", "username"],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "InfoLockDto": {
        "type": "object",
        "properties": {
          "persidNo": {
            "type": "string",
            "description": "Social security number of person that has lock",
            "nullable": true
          },
          "name": {
            "type": "string",
            "description": "Name of person that has lock",
            "nullable": true
          },
          "infoLockType": {
            "type": "string",
            "description": "Info lock code",
            "nullable": true
          },
          "infoLockName": {
            "type": "string",
            "description": "Info lock name",
            "nullable": true
          },
          "invalidDate": {
            "type": "string",
            "description": "When is lock invalid",
            "format": "date-time",
            "nullable": true
          },
          "explanation": {
            "type": "string",
            "description": "Explanation of lock",
            "nullable": true
          },
          "isValid": {
            "type": "boolean",
            "description": "Indicates if the lock is valid or not"
          }
        },
        "additionalProperties": false,
        "description": "Dto for info lock"
      },
      "PostInfoLockModel": {
        "required": ["infoLockType", "persidno"],
        "type": "object",
        "properties": {
          "persidno": {
            "maxLength": 10,
            "minLength": 10,
            "pattern": "^[0-9]{1,10}$",
            "type": "string",
            "description": "Users social security number"
          },
          "infoLockType": {
            "minLength": 1,
            "type": "string",
            "description": "Lock type\r\nAllowed value: 1 (Anonymity) , 2 (Owner registration forbidden)"
          },
          "invalidDate": {
            "type": "string",
            "description": "When is lock invalid",
            "format": "date-time",
            "nullable": true
          },
          "explanation": {
            "maxLength": 100,
            "type": "string",
            "description": "Explanation of lock",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "Entity to insert update lock"
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
