        "required": ["password", "username"],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "PostOrderPlatesModel": {
        "required": ["permno", "stationToDeliverTo", "stationType"],
        "type": "object",
        "properties": {
          "permno": {
            "maxLength": 5,
            "minLength": 5,
            "pattern": "^[A-Z,√û,√ñ]{2}[0-9A-Z]{1}[0-9]{2}",
            "type": "string"
          },
          "frontType": {
            "type": "string",
            "nullable": true
          },
          "rearType": {
            "type": "string",
            "nullable": true
          },
          "stationToDeliverTo": {
            "minLength": 1,
            "pattern": "[1-9]*",
            "type": "string"
          },
          "stationType": {
            "minLength": 1,
            "type": "string"
          },
          "expressOrder": {
            "type": "boolean",
            "nullable": true
          },
          "checkOnly": {
            "type": "boolean",
            "nullable": true
          }
        },
        "additionalProperties": false
