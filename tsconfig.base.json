{
  "compilerOptions": {
    "target": "es2021",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@hxm/api-client/*": ["libs/shared/api-client/src/lib/*"],
      "@hxm/apm": ["libs/apm/src/index.ts"],
      "@hxm/auth/*": ["libs/shared/auth/src/lib/*"],
      "@hxm/constants": ["libs/shared/constants/src/index.ts"],
      "@hxm/db": ["libs/shared/sequelize/src/index.ts"],
      "@hxm/decorators": ["libs/shared/decorators/src/index.ts"],
      "@hxm/logging": ["libs/logging/src/index.ts"],
      "@hxm/middleware": ["libs/shared/middleware/src/index.ts"],
      "@hxm/mocks": ["libs/mocks/src/index.ts"],
      "@hxm/modules": ["libs/shared/modules/src/index.ts"],
      "@hxm/nx-open-next": ["libs/nx-open-next/src/index.ts"],
      "@hxm/pipelines": ["libs/shared/pipelines/src/index.ts"],
      "@hxm/shared/dto": ["libs/shared/dto/src/lib/index.ts"],
      "@hxm/shared/filters": ["libs/shared/filters/src/index.ts"],
      "@hxm/shared/interceptors": ["libs/shared/interceptors/src/index.ts"],
      "@hxm/shared/models/base": ["libs/shared/models/base/src/index.ts"],
      "@hxm/types": ["libs/shared/types/types/src/index.ts"],
      "@hxm/ui/components/*": ["libs/shared/hxm-ui/src/lib/components/*"],
      "@hxm/ui/hooks/*": ["libs/shared/hxm-ui/src/lib/hooks/*"],
      "@hxm/utils": ["libs/shared/utils/src/index.ts"],
      "@hxm/utils/client": ["libs/shared/utils/src/client.ts"],
      "@island.is/island-ui/core": [
        "submodules/island.is/libs/island-ui/core/src/index.ts"
      ],
      "@island.is/island-ui/theme": [
        "submodules/island.is/libs/island-ui/theme/src/index.ts"
      ],
      "@island.is/island-ui/utils": [
        "submodules/island.is/libs/island-ui/utils/src/index.ts"
      ],
      "@island.is/island-ui/vanilla-extract-utils": [
        "submodules/island.is/libs/island-ui/vanilla-extract-utils/src/index.ts"
      ],
      "@island.is/regulations/styling": [
        "submodules/island.is/libs/regulations/src/lib/styling.ts"
      ],
      "@island.is/shared/constants": [
        "submodules/island.is/libs/shared/constants/src/index.ts"
      ],
      "@island.is/shared/types": [
        "submodules/island.is/libs/shared/types/src/index.ts"
      ],
      "@island.is/shared/utils": [
        "submodules/island.is/libs/shared/utils/src/index.ts"
      ]
    },
    "sourceMap": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "Multer"]
  },
  "exclude": ["node_modules", "tmp"]
}
