{
  "version": 2,
  "installCommand": "yarn install",
  "buildCommand": "npx nx build project-api --prod",
  "builds": [
    {
      "src": "apps/project-api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/project-api/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
